{
  "name": "package-proxy",
  "version": "1.2.3",
  "description": "Proxify your npm package for easier imports",
  "files": [
    "lib",
    "bin",
    "LICENSE"
  ],
  "bin": {
    "package-proxy": "bin/index.js",
    "proxify": "bin/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf ./lib && tsc -p .",
    "test": "jest --watch",
    "test:ci": "jest --collectCoverage --ci --silent --no-cache --maxWorkers=3",
    "test:report": "codecov --token=$CODECOV_TOKEN",
    "prepublishOnly": "yarn test:ci && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sl-julienamblard/package-proxy.git"
  },
  "keywords": [
    "package-proxy",
    "cli",
    "package",
    "proxy",
    "proxify"
  ],
  "author": "Julien Amblard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sl-julienamblard/package-proxy/issues"
  },
  "homepage": "https://github.com/sl-julienamblard/package-proxy#readme",
  "devDependencies": {
    "@types/co": "^4.6.3",
    "@types/events": "^3.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/rimraf": "^3.0.2",
    "chalk": "^4.1.2",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "codecov": "^3.8.3",
    "commander": "^8.2.0",
    "cross-env": "^7.0.3",
    "jest": "^27.1.0",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "yargs": "^17.2.1"
  },
  "dependencies": {
    "esm": "^3.2.25"
  }
}
