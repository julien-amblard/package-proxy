{
  "packageName": "consumer",
  "root": "src",
  "proxify": [
    {
      "src": "folderProxy"
    },
    {
      "src": "folderProxy",
      "cjs": "_lib",
      "dest": "customLib"
    },
    {
      "src": "folderProxy",
      "cjs": "_lib",
      "types": "types",
      "dest": "customLibAndCustomTypes"
    },
    {
      "src": "folderProxy",
      "esm": "_esm",
      "dest": "customEsm"
    },
    {
      "src": "folderProxy",
      "dest": "folderProxy_dest"
    },
    {
      "src": "deep/folder",
      "dest": "deepFolder"
    },
    {
      "src": "folderProxy",
      "dest": "deep/folder/dest"
    },
    {
      "src": "filesProxy",
      "proxyType": "file"
    },
    {
      "src": "filesProxy",
      "dest": "filesProxy",
      "proxyType": "file",
      "esm": null
    }
  ]
}
